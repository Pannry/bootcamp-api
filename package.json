{
  "name": "devcamp_api",
  "version": "1.0.0",
  "description": "Devcamper api",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --watch \"./src/\" --exec \"ts-node ./src/server.ts\" -e ts",
    "prod": "npm run build && npm start",
    "test": "jest --no-cache",
    "start": "set NODE_ENV=production&& node ./dist/server",
    "build": "tsc"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.20",
    "@types/morgan": "^1.7.37",
    "@types/supertest": "^2.0.8",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "morgan": "^1.9.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}